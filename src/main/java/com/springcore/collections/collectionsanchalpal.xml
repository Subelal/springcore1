      </set>
   
   </property>
   
   <property name="courses">
   
   <map>
    
    <entry  key="1" value="Bsc-IT"/>
    <entry  key="1" value="MCA"/>
    <entry  key="2" value="B.Com"/>
    <entry  key="3" value="M.com"/>
    <entry  key="4" value="B.Ede"/>
    <entry  key="5" value="BA"/>
   </map>
   
   </property>
   
   <property name="props">
      <props>
         <prop key="name">Anchal</prop>
         <prop key="address">Prayag</prop>
         <prop key="name">Soni</prop>
      </props>
   </property>
 </bean>

</beans>